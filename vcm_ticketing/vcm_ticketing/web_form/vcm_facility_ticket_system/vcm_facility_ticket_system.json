 "client_script": "frappe.web_form.validate = function() {\n    // Get the values of email and mobile number fields\n    var email = frappe.web_form.get_value(\"email_id\");  // Ensure you're using the correct field name\n    var mobile = frappe.web_form.get_value(\"mobile_number\");  // Ensure you're using the correct field name\n\n    // Validate the email\n    var isValidEmail = frappe.utils.validate_type(email, \"email\");\n    if (!isValidEmail) {\n        frappe.msgprint('Invalid email address');  // Show the error message\n        return false;  // Prevent form submission\n    }\n\n    // Validate the mobile number (assuming a 10-digit format)\n    var mobileRegex = /^[0-9]{10}$/;\n    if (frappe.web_form.get_field('mobile_number').df.reqd && !mobileRegex.test(mobile)) {\n        frappe.msgprint('Invalid mobile number. Please enter a 10-digit number.');  // Show the error message\n        return false;  // Prevent form submission\n    }\n\n    return true;  // Allow form submission if both email and mobile are valid\n}\n\n// Handle the 'Save Response' button click\n$(document).on('click', '.btn-form-submit', function(event) {\n    // Trigger form validation\n    if (!frappe.web_form.validate()) {\n        event.preventDefault();  // Stop the form from submitting if validation fails\n    }\n});\n\n\n\n",
